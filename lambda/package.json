{
  "name": "contact-tracing-lambda",
  "version": "1.0.0",
  "private": true,
  "description": "Lambda functions for contact tracing interoperability service",
  "main": "stats.js",
  "scripts": {
    "artifact:create": "zip -r lambda.zip *",
    "batch:all": "env-cmd -f .env node batch.js",
    "batch:complete": "env-cmd -f .env node batch.js \"{ \\\"Records\\\": [] }\"",
    "token:gb": "env-cmd -f .env node token.js \"{ \\\"region\\\": \\\"GB\\\" }\"",
    "token:ie": "env-cmd -f .env node token.js \"{ \\\"region\\\": \\\"IE\\\" }\""
  },
  "dependencies": {
    "@nearform/sql": "^1.4.0",
    "aws-sdk": "^2.646.0",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.0",
    "pg": "^7.18.2",
    "pg-range": "^1.1.0"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "prettier": "prettier-config-standard"
}
